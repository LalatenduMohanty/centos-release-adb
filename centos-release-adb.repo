#Repo for ADB RPMs

[centos7-adb]
name=CentOS-$releasever - Gluster 3.7
baseurl=http://buildlogs.centos.org/centos/$releasever/atomic/$basearch//Packages/
gpgcheck=1
enabled=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-CentOS-SIG-Atomic

[centos7-adb-test]
name=CentOS-$releasever - Gluster 3.7 Testing
baseurl=http://buildlogs.centos.org/centos/$releasever/atomic/$basearch//Packages/
gpgcheck=0
enabled=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-CentOS-SIG-Atomic
