#Repo for ADB RPMs

[centos7-adb]
name=CentOS-$releasever - ADB
baseurl=http://mirror.centos.org/centos/$releasever/atomic/$basearch/adb/
gpgcheck=1
enabled=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-CentOS-SIG-Atomic

[centos7-adb-test]
name=CentOS-$releasever - ADB Testing
baseurl=http://buildlogs.centos.org/centos/$releasever/atomic/$basearch/adb/
gpgcheck=0
enabled=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-CentOS-SIG-Atomic
